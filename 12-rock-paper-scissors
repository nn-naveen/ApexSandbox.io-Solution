public Integer rockPaperScissors(String player1, String player2) {
    if (player1 == player2)
        {
            return 0;
        } else {
            switch (player1){
            case rock:
                if (player2 == scissors)
                    return 1;
                else
                    return 2;
                break;
            case scissors:
                if (player2 == paper)
                    return 1;
                else
                    return 2;
                break;
            case paper:
                if (player2 == rock)
                    return 1;
                else
                    return 2;
                break;
            }
        }
    
}